{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error", {
                "singleQuote": true,
                "trailingComma": "all",
                "arrowParen": "avoid"
            }
        ],
        "no-unused-vars": "warn",
        "no-console": "off",
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off",
        "camelcase":"off",
        "strict": "off"
    }
}
